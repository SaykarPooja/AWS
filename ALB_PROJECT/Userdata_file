#!/bin/bash
sudo su
yum update -y
yum install -y httpd.x86_64
systemctl start httpd.service
systemctl enable httpd.service
echo "<!DOCTYPE html><html><body style='background-color:green'><h1>Welcome to Naresh IT --> Server1</h1></body></html>" >> /var/www/html/index.html

